<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="botonVerde">
        <item name="android:backgroundTint">@color/Teal</item>
        <item name="cornerRadius">15dp</item>
        <item name="android:state_pressed">true</item>
        <item name="android:textColor">@color/blancoApp</item>
        <item name="fontFamily">@font/aeonikbold</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="strokeColor">@color/blancoApp</item>
    </style>

    <style name="botonRojo" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:backgroundTint">@color/colorRojo</item>
        <item name="alpha">1</item>
        <item name="android:state_pressed">true</item>
        <item name="cornerRadius">15dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/aeonikbold</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>



</resources>